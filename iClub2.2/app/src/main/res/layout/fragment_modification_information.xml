            />
        <LinearLayout
            android:id="@+id/oneself_more_layout"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/oneself_more_tv"
                android:textColor="@color/redTextColor"
                android:gravity="center"
                android:text="查看更多"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:layout_marginLeft="3dp"
                android:src="@mipmap/below_return_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>


    </LinearLayout>
    <ImageView
        android:layout_below="@+id/address_layout"
        android:src="@color/lineColor"
        android:layout_width="match_parent"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_marginTop="7dp">
        <ImageView
            android:layout_below="@+id/address_layout"
            android:src="@color/lineColor"
            android:layout_width="match_parent"
            android:layout_height="1dp" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="14dp">



            <TextView
                android:textColor="@color/hunTextColor"
                android:drawablePadding="10dp"
                android:drawableLeft="@mipmap/neself_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="工作经历"
                android:layout_marginLeft="10dp"
                android:textSize="15sp"
                />
            <TextView
                android:id="@+id/modification_work_tv"
                android:layout_alignParentRight="true"
                android:textColor="@color/redTextColor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="编辑"
                android:drawablePadding="10dp"
                android:layout_marginRight="20dp"
                android:textSize="15sp"
                />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#08ffffff"/>

        <TextView
            android:id="@+id/resume_WorkExperience_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
