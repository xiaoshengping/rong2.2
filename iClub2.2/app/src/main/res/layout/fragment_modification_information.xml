            />
        <LinearLayout
            android:id="@+id/experience_more_layout"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/experience_more_tv"
                android:textColor="@color/redTextColor"
                android:gravity="center"
                android:text="查看更多"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:layout_marginLeft="3dp"
                android:src="@mipmap/below_return_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

    </LinearLayout>
    <ImageView
        android:layout_below="@+id/address_layout"
        android:src="@color/lineColor"
        android:layout_width="match_parent"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_marginTop="7dp">
        <ImageView
            android:layout_below="@+id/address_layout"
            android:src="@color/lineColor"
            android:layout_width="match_parent"
            android:layout_height="1dp" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


        <TextView
            android:textColor="@color/hunTextColor"
            android:layout_marginTop="10dp"
            android:drawablePadding="10dp"
            android:drawableLeft="@mipmap/neself_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="联系方式"
            android:layout_marginLeft="25dp"
            android:textSize="15sp"
            />

            <TextView
                android:id="@+id/modification_contact"
                android:layout_alignParentRight="true"
                android:textColor="@color/redTextColor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="编辑"
                android:layout_marginTop="10dp"
                android:layout_marginRight="35dp"
                android:textSize="15sp"
                />


        </RelativeLayout>

        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="QQ"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/textSize14"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/resumeQq_tv"
                android:text="QQ___"
                android:layout_marginLeft="47dp"
                android:textSize="@dimen/textSize14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="Email"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/textSize14"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/resumeEmail_tv"
                android:text="Email"
                android:layout_marginLeft="32dp"
                android:textSize="@dimen/textSize14"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="5dp"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="手机号码"
                android:layout_marginLeft="15dp"
                android:textSize="@dimen/textSize14"
                android:paddingLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/resumeMobile_tv"
                android:text="手机号码"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/textSize14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_marginTop="7dp">
        <ImageView
            android:layout_below="@+id/address_layout"
            android:src="@color/lineColor"
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <LinearLayout
            android:layout_marginTop="20dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/hunTextColor"
                android:text="信誉值"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/textSize14"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/integrity_tv"
                android:text="100"
                android:textColor="@color/redTextColor"
                android:layout_marginLeft="25dp"
                android:textSize="@dimen/textSize14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/hunTextColor"
                android:text="真实性"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/textSize14"
                android:paddingLeft="20dp"
